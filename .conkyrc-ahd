alignment tr
background yes
border_width 1
cpu_avg_samples 2

draw_borders yes
draw_graph_borders yes
draw_outline no
draw_shades no
use_xft yes
xftfont anka/coder condensed-10
#xftfont Allerlight:size=9
gap_x 20
gap_y 170
net_avg_samples 2
double_buffer yes
out_to_console no
out_to_stderr no
extra_newline no
own_window yes
own_window_class Conky
own_window_type desktop
own_window_transparent yes
own_window_argb_visual yes
own_window_argb_value 255
own_window_colour 090909
#own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
stippled_borders 0
update_interval 1.0
uppercase no
use_spacer none
show_graph_scale no
show_graph_range no
format_human_readable yes

#default_color 090909
default_color 000000
default_outline_color white
default_shade_color 282828
color1 111111
color2 353535
color3 5e5e5e
color4 EBEBEB
color5 B6BEC6
color6 159015
color7 35BF5C
color8 30C834
